<!--suppress XmlUnboundNsPrefix -->
<phpunit
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://schema.phpunit.de/5.2/phpunit.xsd"
        bootstrap="ut/bootstrap.php"
>
    <testsuites>
        <testsuite name="all">
            <file>ut/SilexKernelTest.php</file>
            <file>ut/SilexKernelWebTest.php</file>
            <file>ut/CrossOriginResourceSharingTest.php</file>
        </testsuite>
        <testsuite name="security">
            <file>ut/SecurityServiceProviderTest.php</file>
            <file>ut/SecurityServiceProviderConfigurationTest.php</file>
        </testsuite>
    </testsuites>
</phpunit>
